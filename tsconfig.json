{
    "compilerOptions": {
        "baseUrl": "src",
        "target": "es5",
        "module": "commonjs",
        "strict": true,
        "noImplicitAny": false,
        "esModuleInterop": true,
        "allowUnreachableCode": false,
        "sourceMap": true,
        "strictPropertyInitialization": false,
        "lib": [
            "dom",
            "es5",
            "esnext"
        ],
        "paths": {
            "@scenes/*": ["app/scenes/*"],
            "@components/*": ["app/components/*"],
            "@data-set/*": ["app/data-set/*"],
            "@processors/*": ["app/processors/*"]
        }
    }
}
